#  Try to find @PROJECT_NAME@. Once done this will define
#  @PROJECT_NAME@_FOUND        - System has @PROJECT_NAME@
#  @PROJECT_NAME@_INCLUDE_DIRS - The @PROJECT_NAME@ include directories
#  @PROJECT_NAME@_LIBRARIES    - The @PROJECT_NAME@ libraries
#  @PROJECT_NAME@_DEFINITIONS  - Compiler switches required for using @PROJECT_NAME@

# FIXME: is not affected by @PROJECT_NAME@_LIBRARY, @PROJECT_NAME@_INCLUDE_DIR

# FIXME: here it should honor/propagate QUIET and REQUIRED/OPTIONAL
foreach(dep_ @DEPENDS@)
  include(${CMAKE_CURRENT_LIST_DIR}/../${dep_}/${dep_}.cmake)
endforeach()

include(${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@.cmake)

set(@PROJECT_NAME@_LIBRARIES alps::@PROJECT_NAME@)

include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(@PROJECT_NAME@ DEFAULT_MSG @PROJECT_NAME@_LIBRARIES)
